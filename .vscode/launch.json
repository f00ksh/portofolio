{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "portfolio_web",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome", "--wasm"]
    },
    {
      "name": "portfolio_web (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["-d", "chrome", "--wasm"]
    },
    {
      "name": "portfolio_web (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["-d", "chrome", "--wasm"]
    },
    {
      "name": "Flutter for web (hot reloadable)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-experimental-hot-reload",
        "--wasm"
      ]
    }
  ]
}
